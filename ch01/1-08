#include "stdio.h"

int main(){
    int c;
    int line=0;
    int space=0;
    int tab=0;
    while((c=getchar())!=EOF){
        switch (c) {
            case '\n':
                line++;
                break;
            case ' ':
                space++;
                break;
            case '\t':
                tab++;
                break;


        }


    }

    printf("line:%d space:%d tab:%d",line,space,tab);
}
