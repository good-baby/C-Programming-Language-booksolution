#include <stdio.h>
/*when fahr=0.20.40..., print fahr Celsius comparative sheet*/Â 

int main(){
    int fahr;
    float celsius;

    printf(" fahr to celsius sheet\n");
    for (int fahr = 300; fahr >=0 ; ) {
        celsius=5*(fahr-32)/9.0;

        printf("%3d\t%6.1f\n",fahr,celsius);
        fahr-=20;
    }


}
